{
    "name": "miguelcolmenares/contact-form-7-source",
    "description": "Contact Form 7 source for stub generation",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "contact-form-7/contact-form-7",
                "version": "6.1.3",
                "type": "wordpress-plugin",
                "dist": {
                    "url": "https://downloads.wordpress.org/plugin/contact-form-7.6.1.3.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "contact-form-7/contact-form-7": "6.1.3",
        "composer/installers": "^2.0"
    },
    "extra": {
        "installer-paths": {
            "contact-form-7/": ["contact-form-7/contact-form-7"]
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
